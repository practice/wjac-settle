<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
	
<hibernate-mapping package="org.jabberstory.cjac.consignsettle.domain" default-access="field">
	<class name="User" table="USER">
		<id name="userId" column="USERID" type="string" length="32">
			<generator class="assigned" />
		</id>
		<property name="username" column="USERNAME" type="string" length="32" />
		<property name="email" column="EMAIL" type="string" length="64" />
		<property name="password" column="PASSWORD" type="string" length="64" />
		<property name="blocked" column="BLOCKED" type="yes_no" />
		<property name="postnum1" column="POSTNUM1" type="string" length="3" />
		<property name="postnum2" column="POSTNUM2" type="string" length="3" />
		<property name="address" column="ADDRESS" type="string" length="64" />
		<property name="phone1" column="PHONE1" type="string" length="4" />
		<property name="phone2" column="PHONE2" type="string" length="4" />
		<property name="phone3" column="PHONE3" type="string" length="4" />
		<many-to-one name="userGroup" class="UserGroup" column="GROUPID" lazy="false" foreign-key="FK_USER_GROUP"/>
	</class>
	
</hibernate-mapping>
